          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.jsoup.*
import java.util.*
// Parse the response HTML
def html = prev.getResponseDataAsString()
def doc = Jsoup.parse(html)
def rows = doc.select(&quot;table tr&quot;)

// List to hold all valid rows (excluding header)
def validRows = []

for (int i = 1; i &lt; rows.size(); i++) {
    def cols = rows.get(i).select(&quot;td&quot;)
    if (cols.size() &gt;= 6) {
        validRows.add(rows.get(i))
    }
}
if (!validRows.isEmpty()) {
    def random = new Random()
    def randomRow = validRows.get(random.nextInt(validRows.size()))
    def selectedCols = randomRow.select(&quot;td&quot;)

    def flight = selectedCols.get(1).text().trim()
    def airline = selectedCols.get(2).text().trim()
    def price = selectedCols.get(5).text().trim()

    vars.put(&quot;selectedFlight&quot;, flight)
    vars.put(&quot;selectedAirline&quot;, airline)
    vars.put(&quot;selectedPrice&quot;, price)

    // Optional: log to jmeter.log
    log.info(&quot;Randomly Selected Flight: $flight, Airline: $airline, Price: $price&quot;)
}</stringProp>
          </JSR223PreProcessor>
