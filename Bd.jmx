            <stringProp name="script">String[] fromCities = [&quot;Boston&quot;, &quot;Philadelphia&quot;, &quot;San Diego&quot;, &quot;New York&quot;, &quot;London&quot;, &quot;Paris&quot;]
String[] toCities = [&quot;Buenos Aires&quot;, &quot;Rome&quot;, &quot;London&quot;, &quot;Berlin&quot;, &quot;New York&quot;, &quot;Dublin&quot;, &quot;Cairo&quot;]

String fromPort, toPort

Random rand = new Random()

// Keep trying until fromPort and toPort are different
do {
    fromPort = fromCities[rand.nextInt(fromCities.length)]
    toPort = toCities[rand.nextInt(toCities.length)]
} while (fromPort == toPort)

// Set variables to be used in HTTP request
vars.put(&quot;fromPort&quot;, fromPort)
vars.put(&quot;toPort&quot;, toPort)</stringProp>
